
import java.util.*;
import java.io.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    int N = sc.nextInt();
	    int [] a = new int[N];
	    int even = 0;
	    int odd = 0;
	    
	    for(int i=0; i<N; i++){
	        a[i] = sc.nextInt();
	        if(a[i]%2==0){
	            even++;
	        }
	        else{
	            odd++;
	        }
	    }
	    
	    System.out.print(even + " " + odd);
	    
	}
}
