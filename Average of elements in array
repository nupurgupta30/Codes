
import java.util.*;
import java.io.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    int N = sc.nextInt();
	    int [] a = new int[N];
	    
	    for(int i=0; i<N; i++){
	        a[i] = sc.nextInt();
	    }
	    
	    double total = 0;
	    
	    for(int j=0; j<N; j++){
	        total += a[j];
	    }
	    
	    System.out.printf("%.2f", total/N);
	}
}
